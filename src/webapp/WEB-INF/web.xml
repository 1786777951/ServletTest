<?xml version="1.0" encoding="UTF-8" ?>
<web-app >
    <servlet>
        <servlet-name>Hello</servlet-name>
        <servlet-class>main.Hello</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Hello</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <!--登陆-->
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>main.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <!--上传-->
    <servlet>
        <servlet-name>UploadPhotoServlet</servlet-name>
        <servlet-class>main.UploadPhotoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadPhotoServlet</servlet-name>
        <url-pattern>/uploadPhoto</url-pattern>
    </servlet-mapping>
</web-app>